<AnchorPane fx:id="manager_manage_customer_view_pane" prefHeight="600.0" prefWidth="700.0" style="-fx-background-color: #F67280;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ManagerManageCustomerController">
	<children>
		<Button layoutX="32.0" layoutY="41.0" mnemonicParsing="false" onAction="#onManageCustomerBack" prefHeight="27.0" prefWidth="75.0" text="Back">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Button>
		<Button layoutX="574.0" layoutY="41.0" mnemonicParsing="false" onAction="#onManageCustomerExit" prefHeight="27.0" prefWidth="75.0" text="Exit">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Button>
		<TableView fx:id="tblCustomers" layoutX="26.0" layoutY="129.0" prefHeight="451.0" prefWidth="623.0" style="-fx-background-color: #FFDFDD;">
			<columns>
				<TableColumn fx:id="cname" prefWidth="67.0" text="Name" />
				<TableColumn fx:id="ccity" prefWidth="56.0" text="City" />
				<TableColumn fx:id="roomtype" prefWidth="74.0" text="Apt Type" />
				<TableColumn fx:id="roomNo" prefWidth="91.0" text="Apt No" />
				<TableColumn fx:id="noofpeople" prefWidth="82.0" text="No of People" />
				<TableColumn fx:id="roomprice" prefWidth="70.0" text="Apt Price" />
				<TableColumn fx:id="startdate" text="Lease Start Date" />
				<TableColumn fx:id="enddate" prefWidth="100.0" text="Lease End Date" />
			</columns>
		</TableView>
		<Button layoutX="242.0" layoutY="87.0" mnemonicParsing="false" onAction="#onRefreshCustomerList" prefHeight="27.0" prefWidth="175.0" text="Refresh Residency List">
			<font>
				<Font name="System Bold" size="13.0" />
			</font>
		</Button>

	</children>
</AnchorPane>
